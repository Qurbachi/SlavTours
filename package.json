{
  "name": "natours",
  "version": "1.0.0",
  "description": "landing page for natours project",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel watch:sass devserver",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -o css/style.prefix.css css/style.concat.css ",
    "compress:css": "node-sass css/style.prefix.css  css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qurbachi/NatoursProject.git"
  },
  "author": "Damian Tworzyd≈Ço",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Qurbachi/NatoursProject/issues"
  },
  "homepage": "https://github.com/Qurbachi/NatoursProject#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "concat": "^1.0.3",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
